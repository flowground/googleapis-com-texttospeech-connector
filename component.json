{
    "title": "Cloud Text-to-Speech",
    "description": "Synthesizes natural-sounding speech by applying powerful neural network models.",
    "docsUrl": "https://cloud.google.com/text-to-speech/",
    "url": "https://api.apis.guru/v2/specs/googleapis.com/texttospeech/v1/swagger.json",
    "envVars": {
        "OAUTH_CLIENT_ID": {
            "description": "OAuth Client ID"
        },
        "OAUTH_CLIENT_SECRET": {
            "description": "OAuth Client Secret"
        }
    },
    "credentials": {
        "fields": {
            "server": {
                "label": "Server",
                "viewClass": "SelectView",
                "model": [
                    "https://texttospeech.googleapis.com/",
                    "--- Custom URL"
                ],
                "required": true
            },
            "otherServer": {
                "label": "Custom Server URL",
                "viewClass": "TextFieldView"
            },
            "Oauth2": {
                "label": "Oauth 2.0 authentication",
                "viewClass": "OAuthFieldView"
            }
        },
        "oauth2": {
            "client_id": "{{OAUTH_CLIENT_ID}}",
            "client_secret": "{{OAUTH_CLIENT_SECRET}}",
            "auth_uri": "https://accounts.google.com/o/oauth2/auth"
        }
    },
    "triggers": {
        "startFlow": {
            "main": "./lib/triggers/startFlow.js",
            "type": "polling",
            "title": "Start Flow",
            "fields": {}
        }
    },
    "actions": {
        "texttospeech_text_synthesize": {
            "main": "./lib/actions/texttospeech_text_synthesize.js",
            "title": "Synthesizes speech synchronously: receive results after all text input\nhas been processed.",
            "description": "Synthesizes speech synchronously: receive results after all text input\nhas been processed.",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/texttospeech_text_synthesize.in.json",
                "out": {
                    "type": "object"
                }
            }
        },
        "texttospeech_voices_list": {
            "main": "./lib/actions/texttospeech_voices_list.js",
            "title": "Returns a list of Voice supported for synthesis.",
            "description": "Returns a list of Voice supported for synthesis.",
            "fields": {
                "verbose": {
                    "viewClass": "CheckBoxView",
                    "label": "Debug this step (log more data)"
                }
            },
            "metadata": {
                "in": "./lib/schemas/texttospeech_voices_list.in.json",
                "out": {
                    "type": "object"
                }
            }
        }
    }
}